{
    "name": "@inkline/vue",
    "description": "Inkline is the intuitive UI Components library that gives you a developer-friendly foundation for building high-quality, accessible, and customizable Vue.js 3 Design Systems.",
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "devtools vite watch",
        "build": "devtools vite build",
        "lint": "devtools eslint",
        "test": "devtools test",
        "test:dev": "devtools test --watch",
        "test:ci": "devtools test --coverage"
    },
    "devDependencies": {
        "@floating-ui/core": "^1.6.2",
        "@floating-ui/dom": "^1.6.5",
        "@inkline/config": "workspace:*",
        "@inkline/core": "workspace:*",
        "@inkline/devtools": "workspace:*",
        "@inkline/eslint-config": "workspace:*",
        "@inkline/plugin": "workspace:*",
        "@inkline/theme": "workspace:*",
        "@inkline/utils": "workspace:*",
        "@testing-library/jest-dom": "catalog:",
        "@testing-library/vue": "catalog:",
        "@vitest/coverage-v8": "catalog:",
        "@vue/runtime-core": "catalog:",
        "eslint": "catalog:",
        "micromustache": "^8.0.3",
        "miragejs": "0.2.0-alpha.3",
        "normalize.css": "^8.0.1",
        "typescript": "catalog:",
        "vite": "catalog:",
        "vite-plugin-dts": "catalog:",
        "vitest": "catalog:",
        "vue": "catalog:"
    },
    "peerDependencies": {
        "@inkline/config": "workspace:*",
        "@inkline/core": "workspace:*",
        "@inkline/theme": "workspace:*",
        "vue": ">=3.0.0"
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "main": "./dist/vue.umd.cjs",
    "module": "./dist/vue.js",
    "types": "./dist/vue.d.ts",
    "jsdelivr": "dist/vue.umd.js",
    "unpkg": "dist/vue.umd.js",
    "exports": {
        ".": {
            "import": "./dist/vue.js",
            "require": "./dist/vue.umd.cjs",
            "types": "./dist/vue.d.ts"
        },
        "./*": "./*"
    },
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/alexgrozav"
    },
    "bugs": {
        "url": "https://github.com/inkline/inkline/issues"
    },
    "homepage": "https://inkline.io",
    "keywords": [
        "inkline",
        "ui",
        "ux",
        "dx",
        "library",
        "framework",
        "typescript"
    ],
    "engines": {
        "node": "*",
        "pnpm": "*"
    },
    "license": "ISC",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/inkline/inkline.git"
    }
}
