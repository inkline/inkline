.code-preview
    pre
        &:first-child
            margin-top: 0

    p,
    pre,
    table.table,
    .breadcrumb,
    .d1, .d2, .d3, .d4, .d5, .d6
    .h1, .h2, .h3, .h4, .h5, .h6
    h1, h2, h3, h4, h5, h6
        &:last-child
            margin-bottom: 0

    .code-toolbar
        &:last-child pre
            margin-bottom: 0

    .row
        &:last-child .box
            margin-bottom: 0