{
    "name": "@inkline/icons",
    "description": "Inkline Icons provides you with an awesome, maintainable Universal Vue.js 3 Icon Component with thousands of icons from more than 15 icon packs.",
    "version": "1.1.4",
    "main": "build/index.js",
    "module": "./build/index.js",
    "exports": {
        ".": {
            "import": "./build/index.js",
            "require": "./dist/icons.umd.js",
            "sass": "./build/index.scss",
            "style": "./dist/style.css",
            "default": "./build/icons.js"
        },
        "./packs": {
            "import": "./build/packs/index.js",
            "require": "./dist/packs/index.js",
            "sass": "./build/packs/index.scss",
            "default": "./build/packs/index.js"
        },
        "./packs/": {
            "import": "./build/packs/",
            "require": "./dist/packs/",
            "sass": "./build/packs/",
            "default": "./build/packs/"
        },
        "./components": {
            "import": "./build/components/index.js"
        },
        "./controllers": {
            "import": "./build/controllers/index.js"
        },
        "./helpers": {
            "import": "./build/helpers/index.js"
        }
    },
    "types": "./build/index.d.ts",
    "scripts": {
        "dist:plugin": "vue-tsc --noEmit && vite build",
        "dist:packs": "node -r esbuild-register scripts/dist.ts",
        "dist": "npm run build && npm run dist:plugin && npm run dist:packs",
        "build:ts": "tsc -d -p tsconfig.json",
        "build:vue:copy": "rsync -am --include='*.'{vue,scss} --include='*/' --exclude='*' src/ build/",
        "build:vue:replace": "find build -type f -name *.vue -exec perl -i -pe's/ lang=\"ts\"//g' {} +",
        "build:vue": "npm run build:vue:copy && npm run build:vue:replace",
        "build": "npm run generate && npm run build:ts && npm run build:vue",
        "generate:version": "node -r esbuild-register scripts/version.ts",
        "generate:icons": "node -r esbuild-register scripts/generate.ts",
        "generate": "npm run generate:version && npm run generate:icons",
        "prepare": "npm run build"
    },
    "dependencies": {
        "vue": "^3.0.5",
        "@coreui/icons": "2.0.1",
        "@fortawesome/fontawesome-free": "5.15.3",
        "@primer/octicons": "13.0.0",
        "bootstrap-icons": "1.4.1",
        "cryptocurrency-icons": "0.17.1",
        "flag-icon-css": "3.5.0",
        "flat-color-icons": "1.1.0",
        "heroicons": "1.0.1",
        "ionicons": "5.5.1",
        "line-awesome": "1.3.0",
        "material-design-icons": "3.0.1",
        "pixelarticons": "1.4.0",
        "primeicons": "4.1.0",
        "remixicon": "2.5.0",
        "simple-icons": "4.21.0"
    },
    "devDependencies": {
        "@types/estree": "0.0.47",
        "@types/glob": "^7.1.3",
        "@types/node": "^15.0.1",
        "@typescript-eslint/eslint-plugin": "^4.22.0",
        "@typescript-eslint/parser": "^4.22.0",
        "@vitejs/plugin-vue": "^1.2.2",
        "@vue/compiler-sfc": "^3.0.5",
        "chalk": "^4.1.1",
        "esbuild-register": "^2.5.0",
        "eslint": "^7.25.0",
        "eslint-plugin-vue": "^7.9.0",
        "glob": "^7.1.6",
        "sass": "^1.32.12",
        "stylelint-config-standard": "^22.0.0",
        "svgson": "^5.2.1",
        "typescript": "^4.1.3",
        "vite": "^2.2.3",
        "vue-tsc": "^0.0.24"
    },
    "files": [
        "src",
        "build",
        "dist",
        "scripts",
        "README.md",
        "LICENSE",
        "tsconfig.json"
    ],
    "keywords": [
        "vuejs",
        "universal",
        "icon",
        "component",
        "icons",
        "packs",
        "inkline",
        "coreui",
        "cryptocurrency",
        "flag",
        "fontawesome",
        "heroicons",
        "bootstrap",
        "flat color",
        "ionicons",
        "lineawesome",
        "material design",
        "octicons",
        "pixelart",
        "prime",
        "remix",
        "simple"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/inkline/inkline/tree/master/packages/icons"
    },
    "license": "MIT"
}
