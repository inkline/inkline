{
  "name": "@inkline/inkline",
  "version": "1.20.3",
  "description": "Inkline is a modern UI/UX framework for Vue.js, designed for creating flawless content-rich responsive web applications",
  "author": "Alex Grozav <alex@grozav.com>",
  "scripts": {
    "build": "npm run build:inkline && npm run dist",
    "build:inkline": "vue-cli-service build --target lib --name Inkline src/main.js",
    "lint": "npm run lint:script && npm run lint:style",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "dev": "vue-cli-service serve",
    "dist": "node ./scripts/dist.js",
    "lint:script": "vue-cli-service lint",
    "lint:style": "vue-cli-service lint:style",
    "prepublishOnly": "npm run build",
    "test": "vue-cli-service test:unit --coverage",
    "test:dev": "vue-cli-service test:unit --watch"
  },
  "main": "dist/inkline.js",
  "unpkg": "dist/inkline.js",
  "files": [
    "dist/",
    "src/",
    "types/",
    "nuxt/",
    "babel.config.js",
    "LICENSE",
    "package.json",
    "README.md",
    "vue.config.js"
  ],
  "dependencies": {
    "fuse.js": "^3.4.6",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "^1.1.2",
    "@babel/core": "^7.8.4",
    "@babel/types": "^7.8.3",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-unit-jest": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/test-utils": "^1.0.0-beta.31",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^25.1.0",
    "coveralls": "^3.0.9",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2",
    "handlebars": "^4.5.3",
    "node-sass": "^4.13.1",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.14.2",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "peerDependencies": {
    "vue": ">=2.0"
  },
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/inkline"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/inkline/inkline/issues"
  },
  "homepage": "http://inkline.io",
  "jsdelivr": "dist/inkline.js",
  "keywords": [
    "inkline",
    "vue",
    "vuejs",
    "javascript",
    "scss",
    "responsive",
    "front-end",
    "components",
    "ui",
    "ux",
    "framework",
    "validation"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inkline/inkline.git"
  },
  "types": "types/index.d.ts"
}
