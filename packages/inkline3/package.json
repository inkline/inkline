        "src/",
        "types/",
        "nuxt/",
        "babel.config.js",
        "LICENSE",
        "package.json",
        "README.md",
        "vue.config.js"
    ],
    "funding": {
        "type": "patreon",
        "url": "https://www.patreon.com/inkline"
