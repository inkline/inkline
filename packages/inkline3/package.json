        "glob": "^7.1.7",
        "jsdoc-api": "^7.0.1",
        "miragejs": "^0.1.41",
        "postcss-preset-env": "^6.7.0",
        "qs": "^6.10.1",
        "sass": "^1.32.13",
        "stylelint": "^13.13.1",
        "stylelint-config-recommended": "^5.0.0",
        "stylelint-config-recommended-scss": "^4.2.0",
        "stylelint-config-standard": "^22.0.0",
        "stylelint-scss": "^3.18.0",
        "svgson": "^5.2.1",
        "typescript": "^4.2.4",
        "vite": "^2.3.2",
        "vite-plugin-md": "^0.6.6",
        "vue": "^3.0.11",
        "vue-router": "^4.0.8"
    },
    "peerDependencies": {
        "vue": ">=3.0.0"
    },
