        "glob": "^7.1.6",
        "jsdoc-api": "^7.0.0",
        "postcss-preset-env": "^6.7.0",
        "sass": "^1.32.8",
        "stylelint": "^13.9.0",
        "stylelint-config-recommended": "^4.0.0",
        "stylelint-config-recommended-scss": "^4.2.0",
        "stylelint-config-standard": "^21.0.0",
        "stylelint-scss": "^3.18.0",
        "typescript": "^4.1.3",
