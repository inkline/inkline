        "generate:manifest:sass": "node ./scripts/manifest-sass.js",
        "generate:icons": "node ./scripts/icons.js"
    },
    "main": "dist/inkline.js",
    "module": "dist/inkline.js",
    "jsdelivr": "dist/inkline.js",
    "unpkg": "dist/inkline.js",
