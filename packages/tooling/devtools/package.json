{
    "name": "@inkline/devtools",
    "version": "1.0.0",
    "type": "module",
    "bin": {
        "devtools": "dist/cli/main.js"
    },
    "scripts": {
        "dev": "tsx src/cli/main.ts tsup watch",
        "build": "tsx src/cli/main.ts tsup build",
