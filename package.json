{
  "name": "@inkline/plugin",
  "version": "1.4.3",
  "scripts": {
    "start": "npm run build",
    "dev": "npm run dev:vite",
    "dev:vite": "vite",
    "dev:vite:build": "vite build",
    "dev:webpack": "webpack serve",
    "dev:nuxt": "nuxt dev",
    "build": "unbuild",
    "postbuild": "ts-node scripts/postbuild.ts",
    "lint": "eslint --ext .vue,.ts,.tsx,.js,.jsx src",
    "preview": "vite preview",
    "test": "vitest run --passWithNoTests",
    "test:ci": "vitest run --coverage",
    "test:dev": "vitest",
    "prerelease": "npm-run-all test build",
    "release": "np",
    "postrelease": "ts-node scripts/postrelease.ts",
    "postversion": "ts-node scripts/postversion.ts",
    "playground": "ts-node scripts/playground.ts"
  },
  "devDependencies": {
    "@grozav/eslint-config": "latest",
    "@inkline/config": "^1.9.0",
    "@inkline/inkline": "^4.0.0-rc.16",
    "@nuxt/module-builder": "^0.2.0",
    "@nuxt/schema": "^3.0.0",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/shelljs": "0.8.11",
    "chokidar": "^3.5.3",
    "css-loader": "^6.7.1",
    "eslint": "8.31",
    "html-webpack-plugin": "^5.5.0",
    "jsdom": "^21.0.0",
    "np": "^7.6.2",
    "npm-run-all": "4.1.5",
    "nuxt": "^3.3.3",
    "pathe": "^1.0.0",
    "sass": "^1.55.0",
    "sass-loader": "^13.1.0",
    "shelljs": "0.8.5",
    "style-loader": "^3.3.1",
    "ts-node": "10.9.1",
    "typescript": "^4.8.4",
    "unbuild": "^1.0.2",
    "unplugin": "^1.3.1",
    "vite": "^4.0.4",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-inspect": "^0.7.5",
    "vitest": "^0.27.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@grozav/logger": "^1.0.1",
    "@grozav/utils": "^1.1.6",
    "@nuxt/kit": "^3.0.0"
  },
  "peerDependencies": {
    "@inkline/config": "*",
    "@inkline/inkline": "*"
  },
  "main": "index.js",
  "module": "index.mjs",
  "jsdelivr": "index.js",
  "unpkg": "index.js",
  "types": "index.d.ts",
  "files": [
    "**/*"
  ],
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs",
      "types": "./index.d.ts"
    },
    "./esbuild": {
      "require": "./esbuild.js",
      "import": "./esbuild.mjs",
      "types": "./esbuild.d.ts"
    },
    "./nuxt": {
      "require": "./nuxt.js",
      "import": "./nuxt.mjs",
      "types": "./nuxt.d.ts"
    },
    "./rollup": {
      "require": "./rollup.js",
      "import": "./rollup.mjs",
      "types": "./rollup.d.ts"
    },
    "./vite": {
      "require": "./vite.js",
      "import": "./vite.mjs",
      "types": "./vite.d.ts"
    },
    "./webpack": {
      "require": "./webpack.js",
      "import": "./webpack.mjs",
      "types": "./webpack.d.ts"
    },
    "./*": "./*"
  },
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/alexgrozav"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/inkline/inkline/issues"
  },
  "homepage": "https://inkline.io",
  "keywords": [
    "inkline",
    "vue",
    "vuejs",
    "plugin",
    "nuxt",
    "nuxtjs",
    "webpack",
    "vite"
  ],
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inkline/plugin.git"
  }
}
