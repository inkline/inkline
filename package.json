{
    "name": "@inkline/inkline",
    "version": "3.1.8",
    "description": "Inkline is the intuitive UI Components library that gives you a developer-friendly foundation for building high-quality, accessible, and customizable Vue.js 3 Design Systems.",
    "author": "Alex Grozav <alex@grozav.com>",
    "scripts": {
        "dev": "npm run storybook",
        "build": "rimraf lib lib-cjs dist && run-p build:*",
        "build:unbuild": "unbuild",
        "build:vite": "vite build",
        "postbuild": "ts-node ./scripts/postbuild.ts",
        "lint": "run-s lint:*",
        "lint:script": "eslint --ext .vue,.js,.ts src",
        "lint:prettier": "prettier --write --loglevel warn \"**/*.{js,json,css,scss,vue,html}\"",
        "lint:style": "stylelint \"src/**/*.{scss,vue}\"",
        "lint:markdown": "markdownlint '**/*.md'",
        "prerelease": "npm run lint && npm run test && npm run build",
        "release": "np",
        "postrelease": "ts-node scripts/postrelease.ts",
        "postversion": "ts-node scripts/postversion.ts",
        "test": "vitest run --coverage",
        "test:dev": "vitest",
        "test:dev:ui": "vitest --ui",
        "coveralls": "cat coverage/lcov.info | coveralls",
        "generate:config": "inkline generate scss --outputDir ./src/css/variables",
        "generate:utilities": "ts-node ./scripts/generate/utilities.ts",
        "generate:manifest": "ts-node ./scripts/manifest.ts",
        "generate:manifest:sass": "ts-node ./scripts/manifest-sass.ts",
        "storybook": "start-storybook -p 6006",
        "storybook:build": "build-storybook",
        "stylelint-check": "stylelint-config-prettier-check",
        "postinstall": "rimraf ./node_modules/@types/react & rimraf ./node_modules/react/src/**/*.d.ts"
    },
    "main": "inkline.js",
    "module": "inkline.mjs",
    "jsdelivr": "inkline.js",
    "unpkg": "inkline.js",
    "types": "inkline.d.ts",
    "dependencies": {
        "@floating-ui/dom": "^1.0.4",
        "@grozav/utils": "^1.1.6",
        "@popperjs/core": "2.11.6",
        "normalize.css": "^8.0.1"
    },
    "devDependencies": {
        "@babel/core": "^7.19.3",
        "@grozav/eslint-config": "^1.1.4",
        "@grozav/stylelint-config": "^1.0.0",
        "@inkline/cli": "^1.2.1",
        "@inkline/config": "^1.7.4",
        "@storybook/addon-a11y": "^6.5.12",
        "@storybook/addon-actions": "^6.5.12",
        "@storybook/addon-essentials": "^6.5.12",
        "@storybook/addon-interactions": "^6.5.12",
        "@storybook/addon-links": "^6.5.12",
        "@storybook/addon-postcss": "^2.0.0",
        "@storybook/builder-vite": "^0.2.4",
        "@storybook/testing-library": "^0.0.13",
        "@storybook/vue3": "^6.5.12",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/vue": "^6.6.1",
        "@types/jsdom": "^20.0.0",
        "@types/node": "18.8.0",
        "@types/prettier": "^2.7.1",
        "@types/shelljs": "^0.8.11",
        "@vitejs/plugin-vue": "^3.1.2",
        "@vitest/coverage-c8": "^0.25.5",
        "@vitest/ui": "^0.24.3",
        "@vue/eslint-config-typescript": "^11.0.2",
        "@vue/test-utils": "^2.1.0",
        "@vue/tsconfig": "^0.1.3",
        "babel-loader": "^8.2.5",
        "c8": "^7.12.0",
        "esbuild": "^0.16.8",
        "eslint": "^8.25.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-vue": "^9.6.0",
        "fast-glob": "^3.2.12",
        "jsdom": "^20.0.1",
        "markdownlint": "^0.26.2",
        "markdownlint-cli": "^0.32.2",
        "miragejs": "^0.1.45",
        "np": "^7.6.2",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.18",
        "postcss-html": "^1.5.0",
        "postcss-preset-env": "^7.8.2",
        "prettier": "^2.7.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sass": "^1.55.0",
        "shelljs": "^0.8.5",
        "storybook-dark-mode": "^1.1.2",
        "stylelint": "^14.14.0",
        "ts-node": "^10.9.1",
        "typescript": "~4.8.4",
        "unbuild": "^0.9.4",
        "vite": "^3.1.8",
        "vitest": "^0.24.3",
        "vue": "^3.2.41",
        "vue-loader": "^17.0.0",
        "vue-tsc": "^1.0.8"
    },
    "peerDependencies": {
        "vue": ">=3.0.0"
    },
    "files": [
        "**/*"
    ],
    "exports": {
        ".": {
            "require": "./inkline.js",
            "import": "./inkline.mjs",
            "types": "./inkline.d.ts"
        },
        "./components": {
            "require": "./components/index.js",
            "import": "./components/index.mjs",
            "types": "./components/index.d.ts"
        },
        "./constants": {
            "require": "./constants/index.js",
            "import": "./constants/index.mjs",
            "types": "./constants/index.d.ts"
        },
        "./controllers": {
            "require": "./controllers/index.js",
            "import": "./controllers/index.mjs",
            "types": "./controllers/index.d.ts"
        },
        "./directives": {
            "require": "./directives/index.js",
            "import": "./directives/index.mjs",
            "types": "./directives/index.d.ts"
        },
        "./helpers": {
            "require": "./helpers/index.js",
            "import": "./helpers/index.mjs",
            "types": "./helpers/index.d.ts"
        },
        "./i18n": {
            "require": "./i18n/index.js",
            "import": "./i18n/index.mjs",
            "types": "./i18n/index.d.ts"
        },
        "./mixins": {
            "require": "./mixins/index.js",
            "import": "./mixins/index.mjs",
            "types": "./mixins/index.d.ts"
        },
        "./transitions": {
            "require": "./transitions/index.js",
            "import": "./transitions/index.mjs",
            "types": "./transitions/index.d.ts"
        },
        "./validation": {
            "require": "./validation/index.js",
            "import": "./validation/index.mjs",
            "types": "./validation/index.d.ts"
        },
        "./*": "./*"
    },
    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/alexgrozav"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "bugs": {
        "url": "https://github.com/inkline/inkline/issues"
    },
    "homepage": "https://inkline.io",
    "keywords": [
        "inkline",
        "vue",
        "vuejs",
        "ui",
        "ux",
        "dx",
        "components",
        "library",
        "framework",
        "typescript",
        "javascript",
        "scss",
        "responsive",
        "front end",
        "validation"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/inkline/inkline.git"
    }
}
