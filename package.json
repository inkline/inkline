{
    "name": "@inkline/inkline",
    "version": "3.0.0-beta.21",
    "description": "Inkline is the Vue.js UI/UX Library built for creating your next design system",
    "author": "Alex Grozav <alex@grozav.com>",
    "scripts": {
        "dev": "vite",
        "build:ts:compile": "tsc -d -p tsconfig.json && tsconfig-replace-paths --project tsconfig.json",
        "build:ts:copy": "bash -c 'rsync -am --include=\"*.\"{vue,scss,html,js,md,d.ts} --include=\"*/\" --exclude=\"*\" src/ lib/'",
        "build:ts:replace": "bash -c 'find lib -type f -name *.vue -exec perl -i -pe\"s/.ts\\\" lang=\\\"ts\\\"/.js\\\"/g\" {} +'",
        "build:ts": "npm run build:ts:compile && npm run build:ts:copy && npm run build:ts:replace",
        "build:vite": "vite build",
        "build": "npm run build:ts && npm run build:vite",
        "postbuild": "ts-node scripts/postbuild.ts",
        "lint": "npm run lint:script && npm run lint:style",
        "coveralls": "cat coverage/lcov.info | coveralls",
        "lint:script": "eslint --ext .vue,.js src",
        "lint:style": "echo 'stylelint not installed'",
        "prepublishOnly": "npm run build",
        "test": "",
        "test:dev": "jest --watch",
        "generate:manifest": "ts-node ./scripts/manifest.ts",
        "generate:manifest:sass": "ts-node ./scripts/manifest-sass.ts"
    },
    "main": "inkline.js",
    "module": "inkline.js",
    "jsdelivr": "dist/inkline.umd.js",
    "unpkg": "dist/inkline.umd.js",
    "types": "index.d.ts",
    "dependencies": {
        "@inkline/icons": "^1.4.5",
        "@popperjs/core": "2.9.2",
        "fuse.js": "6.4.0"
    },
    "devDependencies": {
        "@babel/eslint-parser": "^7.14.7",
        "@types/fs-extra": "^9.0.12",
        "@types/node": "^16.6.2",
        "@typescript-eslint/eslint-plugin": "^4.29.1",
        "@typescript-eslint/parser": "^4.29.1",
        "@vitejs/plugin-vue": "^1.2.5",
        "@vue/compiler-sfc": "3.1",
        "@vue/eslint-config-typescript": "^7.0.0",
        "eslint": "^7.32.0",
        "eslint-config-alloy": "^4.1.0",
        "eslint-plugin-vue": "^7.16.0",
        "fs-extra": "^10.0.0",
        "glob": "7.1.7",
        "jsdoc-api": "7.0.1",
        "miragejs": "0.1.41",
        "postcss-preset-env": "6.7.0",
        "qs": "6.10.1",
        "sass": "1.35",
        "stringify-object": "3.3.0",
        "stylelint": "13.13.1",
        "stylelint-config-recommended": "5.0.0",
        "stylelint-config-recommended-scss": "4.3",
        "stylelint-config-standard": "22.0.0",
        "stylelint-scss": "3.20",
        "svgson": "5.2.1",
        "ts-node": "^10.2.0",
        "tsconfig-replace-paths": "^0.0.5",
        "typescript": "4.3",
        "vite": "2.4",
        "vite-plugin-eslint": "^1.3.0",
        "vite-plugin-md": "^0.8.0",
        "vue": "3.1",
        "vue-eslint-parser": "^7.9.0",
        "vue-router": "^4.0.10"
    },
    "peerDependencies": {
        "vue": ">=3.0.0"
    },
    "files": [
        ".npm/**/*"
    ],
    "funding": {
        "type": "patreon",
        "url": "https://www.patreon.com/inkline"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 8"
    ],
    "bugs": {
        "url": "https://github.com/inkline/inkline/issues"
    },
    "homepage": "http://inkline.io",
    "keywords": [
        "inkline",
        "vue",
        "vuejs",
        "ui",
        "ux",
        "dx",
        "components",
        "library",
        "framework",
        "typescript",
        "javascript",
        "scss",
        "responsive",
        "front end",
        "validation"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/inkline/inkline.git"
    }
}
