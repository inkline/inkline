{
  "name": "@inkline/inkline",
  "version": "3.0.0-beta.31",
  "description": "Inkline is the Vue.js UI/UX Library built for creating your next design system",
  "author": "Alex Grozav <alex@grozav.com>",
  "scripts": {
    "dev": "npm run storybook",
    "build:ts:compile": "tsc -d -p tsconfig.json && tsconfig-replace-paths --project tsconfig.json",
    "build:ts:copy": "bash -c 'rsync -am --include=\"*.\"{vue,scss,html,js,md,d.ts} --include=\"*/\" --exclude=\"*\" src/ lib/'",
    "build:ts:replace": "bash -c 'find lib -type f -name *.vue -exec perl -i -pe\"s/.ts\\\" lang=\\\"ts\\\"/.js\\\"/g\" {} +'",
    "build:ts:tests": "find lib -name *.spec.js -type f -delete && find lib -name *.spec.d.ts -type f -delete",
    "build:ts": "npm run build:ts:compile && npm run build:ts:copy && npm run build:ts:replace && npm run build:ts:tests",
    "build": "npm run build:ts",
    "dist": "npm run build && vite build",
    "postdist": "ts-node scripts/postdist.ts",
    "lint": "npm run lint:script && npm run lint:style",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "lint:script": "eslint --ext .vue,.js,.ts src",
    "lint:style": "stylelint \"src/**/*.scss\"",
    "prerelease:sourcemaps": "cp -r src package/src && bash -c 'find package -type f -name *.js.map -exec perl -i -pe\"s/..\\/src/src/g\" {} +'",
    "prerelease": "npm run dist && ts-node scripts/prepublish.ts && npm run prerelease:sourcemaps",
    "release": "np",
    "postrelease": "ts-node scripts/postpublish.ts",
    "postversion": "ts-node scripts/postversion.ts",
    "test": "jest --coverage",
    "test:dev": "jest --watch",
    "generate:manifest": "ts-node ./scripts/manifest.ts",
    "generate:manifest:sass": "ts-node ./scripts/manifest-sass.ts",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook"
  },
  "main": "index.js",
  "module": "index.js",
  "jsdelivr": "dist/inkline.umd.js",
  "unpkg": "dist/inkline.umd.js",
  "types": "index.d.ts",
  "dependencies": {
    "@inkline/icons": "^1.4.13",
    "@popperjs/core": "2.9.2",
    "fuse.js": "6.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.0",
    "@storybook/addon-a11y": "^6.3.12",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/vue3": "^6.3.12",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/vue": "^6.5.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "3.1",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.16",
    "babel-loader": "^8.2.3",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-vue": "^8.0.3",
    "fs-extra": "^10.0.0",
    "glob": "7.1.7",
    "jest": "^26.6.3",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "jsdoc-api": "7.0.1",
    "miragejs": "0.1.41",
    "np": "^7.5.0",
    "postcss-preset-env": "6.7.0",
    "qs": "6.10.1",
    "rimraf": "^3.0.2",
    "sass": "1.35",
    "storybook-builder-vite": "^0.1.5",
    "stringify-object": "3.3.0",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-config-recommended-scss": "4.3",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-standard-scss": "^2.0.1",
    "stylelint-scss": "^3.20.1",
    "svgson": "5.2.1",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.4.0",
    "tsconfig-replace-paths": "^0.0.5",
    "tslib": "^2.3.1",
    "typescript": "4.3",
    "vite": "^2.4.4",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-md": "^0.8.0",
    "vue": "3.1",
    "vue-eslint-parser": "^7.11.0",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-loader": "^16.8.3",
    "vue-router": "^4.0.12"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "files": [
    "**/*"
  ],
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/inkline"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "bugs": {
    "url": "https://github.com/inkline/inkline/issues"
  },
  "homepage": "http://inkline.io",
  "keywords": [
    "inkline",
    "vue",
    "vuejs",
    "ui",
    "ux",
    "dx",
    "components",
    "library",
    "framework",
    "typescript",
