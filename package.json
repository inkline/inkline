{
    "name": "inkline",
    "version": "1.0.0",
    "description": "Inkline monorepo",
    "main": "index.js",
    "scripts": {
        "postinstall": "turbo run build --filter @inkline/devtools",
        "build": "turbo run build --filter !@inkline/e2e*",
        "dev": "turbo run dev --concurrency 16 --filter !@inkline/e2e* --filter !@inkline/component* --filter !@inkline/addon* --filter !@inkline/logger --filter !@inkline/nuxt --filter !@inkline/vite --filter !@inkline/storybook",
        "storybook": "cd storybook && pnpm run dev",
        "test": "turbo run test",
        "lint": "turbo run lint"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/inkline/inkline.git"
    },
    "keywords": [
        "inkline"
    ],
    "author": "Alex Grozav <alex@grozav.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/inkline/inkline/issues"
    },
    "homepage": "https://github.com/inkline/inkline#readme",
    "devDependencies": {
        "prettier": "catalog:",
        "turbo": "catalog:"
    },
    "packageManager": "pnpm@9.2.0"
}
