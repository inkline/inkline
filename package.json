{
  "name": "@inkline/inkline",
  "version": "3.0.0-rc.3",
  "description": "Inkline is the Vue.js UI/UX Library built for creating your next design system",
  "author": "Alex Grozav <alex@grozav.com>",
  "scripts": {
    "dev": "npm run storybook",
    "build:ts:compile": "tsc -d -p tsconfig.json && tsconfig-replace-paths --project tsconfig.json",
    "build:ts:copy": "bash -c 'rsync -am --include=\"*.\"{vue,scss,html,js,md,d.ts} --include=\"*/\" --exclude=\"*\" src/ lib/'",
    "build:ts:replace": "bash -c 'find lib -type f -name *.vue -exec perl -i -pe\"s/.ts\\\" lang=\\\"ts\\\"/.js\\\"/g\" {} +'",
    "build:ts:delete:tests": "find lib -name __tests__ -type d -exec rm -rf {} +",
    "build:ts:delete:storybook": "rm -r lib/__storybook__ && find lib -name index.stories.* -type f -delete",
    "build:ts:delete": "npm run build:ts:delete:tests && npm run build:ts:delete:storybook",
    "build:ts": "npm run build:ts:compile && npm run build:ts:copy && npm run build:ts:replace && npm run build:ts:delete",
    "build": "npm run build:ts",
    "dist": "npm run build && vite build",
    "postdist": "ts-node scripts/postdist.ts",
    "lint": "npm run lint:script && npm run lint:style",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "lint:script": "eslint --ext .vue,.js,.ts src",
    "lint:style": "stylelint \"src/**/*.scss\"",
    "prerelease:sourcemaps": "cp -r src package/src && bash -c 'find package -type f -name *.js.map -exec perl -i -pe\"s/..\\/src/src/g\" {} +'",
    "prerelease": "npm run lint && npm run test && npm run dist && ts-node scripts/prepublish.ts && npm run prerelease:sourcemaps",
    "release": "np",
    "postrelease": "ts-node scripts/postpublish.ts",
    "postversion": "ts-node scripts/postversion.ts",
    "test": "jest --coverage",
    "test:dev": "jest --watch",
    "generate:manifest": "ts-node ./scripts/manifest.ts",
    "generate:manifest:sass": "ts-node ./scripts/manifest-sass.ts",
    "storybook": "start-storybook -p 6006",
    "build:storybook": "build-storybook",
    "build-storybook": "build-storybook"
  },
  "main": "dist/inkline.js",
  "module": "inkline.js",
  "jsdelivr": "dist/inkline.umd.js",
  "unpkg": "dist/inkline.umd.js",
  "types": "inkline.d.ts",
  "dependencies": {
    "@inkline/icons": "1.4.13",
    "@popperjs/core": "2.11.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.5",
    "@storybook/addon-a11y": "6.4.9",
    "@storybook/addon-actions": "6.4.9",
    "@storybook/addon-essentials": "6.4.9",
    "@storybook/addon-links": "6.4.9",
    "@storybook/addon-postcss": "2.0.0",
    "@storybook/builder-webpack5": "6.4.9",
    "@storybook/manager-webpack5": "6.4.9",
    "@storybook/vue3": "6.4.9",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/vue": "6.5.0",
    "@types/fs-extra": "9.0.13",
    "@types/jest": "26.0.24",
    "@types/node": "16.11.12",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "@vitejs/plugin-vue": "2.0.0",
    "@vue/compiler-sfc": "3.2.26",
    "@vue/eslint-config-typescript": "9.1.0",
    "@vue/test-utils": "2.0.0-rc.16",
    "@vue/vue3-jest": "27.0.0-alpha.4",
    "babel-loader": "8.2.3",
    "comment-parser": "1.3.0",
    "eslint": "8.4.1",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "eslint-plugin-vue": "8.2.0",
    "fs-extra": "10.0.0",
    "glob": "7.2.0",
    "html-loader": "3.0.1",
    "jest": "26.6.3",
    "jest-serializer-vue": "2.0.2",
    "jest-transform-stub": "2.0.0",
    "miragejs": "0.1.43",
    "np": "7.6.0",
    "postcss": "8.4.5",
    "postcss-preset-env": "7.0.1",
    "qs": "6.10.1",
    "raw-loader": "4.0.2",
    "rimraf": "3.0.2",
    "sass": "1.45.0",
    "sass-loader": "12.4.0",
    "stringify-object": "3.3.0",
    "stylelint": "14.1.0",
    "stylelint-config-recommended": "6.0.0",
    "stylelint-config-recommended-scss": "5.0.2",
    "stylelint-config-standard": "24.0.0",
    "stylelint-config-standard-scss": "3.0.0",
    "stylelint-processor-html": "1.0.0",
    "stylelint-scss": "4.0.1",
    "svgson": "5.2.1",
    "ts-jest": "26.5.6",
    "ts-node": "10.4.0",
    "tsconfig-paths-webpack-plugin": "3.5.2",
    "tsconfig-replace-paths": "0.0.11",
    "tslib": "2.3.1",
    "typescript": "4.5.4",
    "vite": "2.7.2",
    "vite-plugin-eslint": "1.3.0",
    "vite-plugin-md": "0.11.6",
    "vue": "3.2.20",
    "vue-eslint-parser": "8.0.1",
    "vue-jest": "5.0.0-alpha.10",
    "vue-loader": "16.8.3",
    "vue-router": "4.0.12"
  },
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "files": [
    "**/*"
  ],
  "funding": {
